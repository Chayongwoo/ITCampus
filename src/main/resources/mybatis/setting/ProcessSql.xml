<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="process">
	<insert id="insertProcess" parameterType="processdto">
		insert into process(process_type,process_member_num,process_subject,process_intr,process_peoples,process_startdate,process_enddate,process_teacher,process_writeday) values (#{process_type},#{process_member_num},#{process_subject},#{process_intr},#{process_peoples},#{process_startdate},#{process_enddate},#{process_teacher},now())
	</insert>
	<select id="searchTeacher" resultType="memberdto">
		select * from member where member_type = "강사"
	</select>
	<select id="selectProcessMaxnum" resultType="int">
		select max(process_num) from process
	</select>
</mapper>