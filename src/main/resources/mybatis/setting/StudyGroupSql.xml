<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="studygroup">
	<insert id="insertOfStudyGroup" parameterType="HashMap">
		INSERT INTO studygroup(studygroup_study_num,studygroup_member_num) VALUES (#{studygroup_study_num},#{studygroup_member_num})
	</insert>
	<insert id="insertOfStudyGroupByDto" parameterType="studygroupdto">
		INSERT INTO studygroup(studygroup_study_num,studygroup_member_num) VALUES (#{studygroup_study_num},#{studygroup_member_num})
	</insert>
	<select id="selectCountOfStudyGroupPeoples" parameterType="int" resultType="int">
		SELECT COUNT(*) as count FROM studygroup WHERE studygroup_study_num = #{studygroup_study_num}
	</select>
</mapper>