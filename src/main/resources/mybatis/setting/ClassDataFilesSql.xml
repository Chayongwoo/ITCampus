<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="classdatafiles">
<insert id="insertOfClassDataFiles" parameterType="classdatafilesdto">
		insert into classdatafiles(
		classdatafiles_classdata_num,classdatafiles_classdata_filename)
		values (#{classdatafiles_classdata_num},#{classdatafiles_classdata_filename})
	</insert>

</mapper>